{
  "layer": "top",
  "exclusive": true,
  "position": "top",
  "height": 18,
  "margin": "2 2 0 2",
  "spacing": 1,
  "gtk-layer-shell": true,
  "reload_style_on_change": true,
  "cursor": "pointer",
  "modules-left": ["hyprland/workspaces","hyprland/window"],
  "modules-center": ["clock"],
  "modules-right": ["custom/cpu_power","group/cpu_temp","custom/memory","pulseaudio","backlight","network","battery","tray"],
  "hyprland/workspaces": {
    "format": "{id}",
    "on-click": "activate",
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "persistent-workspaces": {"*": 10},
    "show-special": false,
    "all-outputs": false
  },
  "hyprland/window": {
    "max-length": 60,
    "separate-outputs": true,
    "rewrite": {
      "(.*) ‚Äî Mozilla Firefox": "Firefox: $1",
      "(.*) - NeoVim": "Vim: $1",
      "(.*) - Visual Studio Code": "Code: $1"
    }
  },
  "clock": {
    "format": "{:%a %H:%M}",
    "tooltip": true,
    "tooltip-format": "{:%A, %B %d, %Y}\nWeek %V"
  },
  "custom/cpu_power": {
    "exec": "/home/r2unit/.config/hypr/waybar/scripts/cpu_power.sh",
    "return-type": "json",
    "interval": 2,
    "format": "{}",
    "tooltip": true
  },
  "custom/cpu": {
    "exec": "/home/r2unit/.config/hypr/waybar/scripts/cpu_usage.sh",
    "return-type": "json",
    "interval": 2,
    "format": "CPU {}",
    "tooltip": true
  },
  "custom/memory": {
    "exec": "/home/r2unit/.config/hypr/waybar/scripts/memory_usage.sh",
    "return-type": "json",
    "interval": 2,
    "format": "MEM {}",
    "tooltip": true
  },
  "group/cpu_temp": {
    "orientation": "horizontal",
    "modules": ["custom/cpu","temperature"]
  },
  "temperature": {
